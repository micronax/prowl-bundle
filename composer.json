{
    "name": "micronax/prowl-bundle",
    "description": "Share the xenji/prowlPHP lib in a symfony service container",
    "type": "symfony-bundle",
    "keywords": ["symfony2", "prowl", "prowlphp"],
    "homepage": "https://github.com/Micronax/prowl-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian Golle",
            "email": "me@fabian-golle.de"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "xenji/ProwlPHP": ">=1.0.2"
    },
    "target-dir": "micronax/prowl-bundle",
    "autoload": {
        "psr-0": {
            "micronax\\prowl-bundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
